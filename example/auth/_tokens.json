{
  "name": "_tokens",
  "description": "Collection that contains all tokens of the users",
  "properties": {
    "token": {
      "type": "string",
      "required": true
    },
    "userId": {
      "type": "string",
      "required": true
    }
  },
  "relations":{
    "user": {
      "type": "belongsTo",
      "collection": "_users",
      "foreignKey": "userId"
    }
  }
}
