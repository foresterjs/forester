{
  "name": "_tokens",
  "description": "Collection that contains all tokens of the users",
  "properties": {
    "token": {
      "type": "string"
    },
    "datetime": {
      "type": "date"
    },
    "ip": {
      "type": "string"
    },
    "valid": {
      "type": "boolean"
    }
  },
  "relations":{
    "user": {
      "type": "belongsTo",
      "collection": "_users",
      "foreignKey": "userId"
    }
  }
}
